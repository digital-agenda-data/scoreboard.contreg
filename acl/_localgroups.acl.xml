<?xml version="1.0" encoding="UTF-8"?>
<acl description="ACL for managing localgroups">
    <entries>
        <entry type="object">
            <principal id="cr_admin" type="localgroup"/>
            <permissions>
                <permission id="u"/>
                <permission id="c"/>
                <permission id="v"/>
            </permissions>
        </entry>
        <entry type="object">
            <principal id="roug" type="user"/>
            <permissions>
                <permission id="u"/>
                <permission id="v"/>
            </permissions>
        </entry>
        <entry type="object">
            <principal id="authenticated" type="user"/>
            <permissions>
                <permission id="v"/>
            </permissions>
        </entry>
        <entry type="object">
            <principal id="anonymous" type="user"/>
            <permissions>
                <permission id="v"/>
            </permissions>
        </entry>
    </entries>
</acl>

